<StimulationSet>

<!-- Da pulse to produce 0.5 uM alone -->
<InjectionStim specieID="DaDbuf" injectionSite="sa1[0].pointA">
        <onset>               20000          </onset>
        <duration>            15              </duration>
        <rate>                18            </rate>
        <period>              10000             </period>
        <end>                 602000          </end>
</InjectionStim>

<!-- basal ACh value prior to and after stim  -->
<InjectionStim specieID="ACh" injectionSite="dend:submembrane">
        <onset>               0          </onset>
        <duration>            20000      </duration>
        <rate>                0.2         </rate>
</InjectionStim>
<InjectionStim specieID="ACh" injectionSite="dend:submembrane">
        <onset>               602000          </onset>
        <duration>            100000      </duration>
        <rate>                0.2         </rate>
</InjectionStim>

<!-- basal ACh stim restored after each pulse-->
<InjectionStim specieID="ACh" injectionSite="dend:submembrane">
        <onset>               20400          </onset>
        <duration>             9600        </duration>
        <rate>                0.2           </rate>
        <period>              10000           </period>
        <end>                 602000          </end>
</InjectionStim>

<InjectionStim specieID="ACh" injectionSite="sa1[0].pointA">
        <onset>               20000          </onset>
        <duration>            15              </duration>
        <rate>                3.0           </rate>
        <period>              10000             </period>
        <end>                 602000          </end>
</InjectionStim>

<InjectionStim specieID="ACh" injectionSite="dend:submembrane">
        <onset>               20000          </onset>
        <duration>            15              </duration>
        <rate>                3.0           </rate>
        <period>              10000             </period>
        <end>                 602000          </end>
</InjectionStim>

<!-- Stimulation rate is 0.1 Hz, 60 pairings takes 600 sec -->
<InjectionStim specieID="GluGbuf" injectionSite="sa1[0].pointA">
        <onset>               20000          </onset>
        <duration>            5              </duration>
        <rate>                5           </rate>
        <period>              10000             </period>
        <end>                 602000          </end>
</InjectionStim>

<!-- forward pairing implemented with higher calcium injection -->
<InjectionStim specieID="Ca" injectionSite="sa1[0].pointA">
        <onset>               20000          </onset>
        <duration>            60              </duration>
        <rate>                312            </rate>
        <period>              10000             </period>
        <end>                 602000          </end>
</InjectionStim>

<InjectionStim specieID="Ca" injectionSite="dend:submembrane">
