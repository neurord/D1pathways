<StimulationSet>

<!-- Da pulse to produce 0.5 uM alone.  Should this be decremented 50% over time? -->
<InjectionStim specieID="DaDbuf" injectionSite="sa1[0].pointA">
        <onset>               20000          </onset>
        <duration>            15              </duration>
        <rate>                18            </rate>
</InjectionStim>

<InjectionStim specieID="DaDbuf" injectionSite="sa1[0].pointA">
        <onset>               21000          </onset>
        <duration>            15              </duration>
        <rate>                9            </rate>
        <period>              1000             </period>
        <end>                 79999          </end>
</InjectionStim>

<!-- basal ACh value prior to and after stim  -->
<InjectionStim specieID="ACh" injectionSite="dend:submembrane">
        <onset>               0          </onset>
        <duration>            20000      </duration>
        <rate>                0.2         </rate>
</InjectionStim>
<InjectionStim specieID="ACh" injectionSite="dend:submembrane">
        <onset>               80000          </onset>
        <duration>            220000      </duration>
        <rate>                0.2         </rate>
</InjectionStim>

<!-- basal ACh stim restored after each pulse-->
<InjectionStim specieID="ACh" injectionSite="dend:submembrane">
        <onset>               20400          </onset>
        <duration>              600        </duration>
        <rate>                0.2           </rate>
        <period>              1000           </period>
        <end>                 79999          </end>
</InjectionStim>

<InjectionStim specieID="ACh" injectionSite="sa1[0].pointA">
        <onset>               20000          </onset>
        <duration>            15              </duration>
        <rate>                3.0           </rate>
        <period>              1000             </period>
        <end>                 79999          </end>
</InjectionStim>

<InjectionStim specieID="ACh" injectionSite="dend:submembrane">
        <onset>               20000          </onset>
        <duration>            15              </duration>
        <rate>                3.0           </rate>
        <period>              1000             </period>
        <end>                 79999          </end>
</InjectionStim>

<!-- Stimulation rate is 1 Hz, 60 pairings takes 60 sec -->
<InjectionStim specieID="GluGbuf" injectionSite="sa1[0].pointA">
        <onset>               20000          </onset>
        <duration>            5              </duration>
        <rate>                5           </rate>
        <period>              1000             </period>
        <end>                 79999          </end>
</InjectionStim>

<!-- forward pairing implemented with higher calcium injection -->
